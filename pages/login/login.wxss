/*底*/
.page {                              
    display: flex;
    flex-direction: column;
    align-items:center;
}
/*图片*/
.login-image {   
  margin-top: -100rpx;                          
  width: 750rpx;
}
/*输入+记住账号密码*/
.login-wrap {                       
margin-top: 100rpx;
width: 70%;
}
.login-btn {
    color: rgb(255, 255, 255);
    background: rgb(255, 180, 20);
    height: 80%;
    line-height: 82rpx;
    width: 70%;
    margin-top: 20rpx;
    border-radius: 30px;
}
.weui-input {
    font-size: 36rpx;
    text-align: left;
    padding: 2rpx 0 2rpx 10rpx;
}
.weui-label {
    width: 100rpx;
}
.remenber {
  margin-top: 20rpx;
    float:right;
}
.remenber .weui-agree__checkbox-icon {
    width:14px;
    height:14px;
}
.remenber .weui-agree__checkbox-icon-check {
    width: 100%;
    height: 100%;
}
.endtitle{
  position: fixed;
  font-size: 30rpx;
  color: rgb(0, 0, 0);
  bottom:40rpx;

}
